import math



u = input("Digite o valor da media")
sigma = float(input("Digite o valor do desvio padr√£o"))

nzin = float(input("Digite o valor da amostra"))

nzao = float(input("Digite o valor total"))

if nzao == 0:
    total = sigma /math.sqrt(nzin)
else:
    total = (sigma /math.sqrt(nzin))*((math.sqrt(nzao - nzin))/(math.sqrt(nzao-1)))


print("%.3f"%total)



